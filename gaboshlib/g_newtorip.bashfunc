#!/bin/bash

function g_newtorip {
 g_echo "renewing Tor-IP"
 g_iptor
 g_echo "current Tor-IP $g_iptor"
 (echo authenticate '""'; echo signal newnym; echo quit) | nc $g_tor_host 9051 || g_echo_error "failed connection to tor service"
 g_echo "waiting 10 seconds"
 sleep 10
 g_iptor
 g_echo "new Tor-IP IP $g_iptor"
}

